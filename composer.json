{
	"require": {
		"illuminate/database": "~6.20.12",
		"php-di/php-di": "^6.2",
		"selective/basepath": "^1.0",
		"slim/php-view": "^2.2",
		"slim/psr7": "^1.1",
		"slim/slim": "^4.5",
		"slim/twig-view": "^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Test\\": "tests/"
		}
	},
	"config": {
		"process-timeout": 0,
		"sort-packages": true
	}
}
